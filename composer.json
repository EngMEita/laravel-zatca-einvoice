{
    "name": "meita/laravel-zatca-einvoice",
    "description": "ZATCA E-Invoicing (Fatoora) integration package for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Adel Eita",
            "email": "contact@mohammedadel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Meita\\ZatcaEInvoice\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meita\\ZatcaEInvoice\\ZatcaInvoiceServiceProvider"
            ],
            "aliases": {
                "ZatcaInvoice": "Meita\\ZatcaEInvoice\\Facades\\ZatcaInvoice"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}